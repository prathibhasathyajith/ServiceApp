            <result name="resetpassword">/defaultaccesscontroler.jsp</result>
            <result name="logoutuser" type="json"></result>
            <result name="view">/WEB-INF/pages/usermanagement/passwordreset.jsp</result>
            <result name="success"></result>
        </action>
        
        
        
    </package>
